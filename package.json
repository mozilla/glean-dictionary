    "@fullhuman/postcss-purgecss": "^2.3.0",
    "just-throttle": "^1.1.0",
    "lunr": "^2.3.9",
    "node-fetch": "^2.6.0",
    "postcss": "^7.0.32",
    "postcss-load-config": "^2.1.0",
